s=input('Enter the string')
k=int(input('Enter the no'))
l=[]


def palind(s,k):
    c=0
    j=len(s)-1
    st=s
    
    i=0
   
    while i<j:
        if s[i]==s[j]:
            i=i+1
            j=j-1
        elif s[i]!=s[j]:
            j=j-1
            c=c+1
            
                
                
    if c<=k:
        return 'True'
    else:
        return 'False'
    
  
        
print(palind(s,k))
    
